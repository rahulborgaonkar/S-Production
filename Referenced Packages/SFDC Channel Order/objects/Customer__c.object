    <validationRules>
        <fullName>Country_2_letter_code</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(REGEX(  Customer_Country__c  , &quot;[A-Z]{2}&quot;))</errorConditionFormula>
        <errorDisplayField>Customer_Country__c</errorDisplayField>
        <errorMessage>Invalid Country - Country cannot contain special characters or numbers, Country is a two letter code.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invalid_OrgID</fullName>
        <active>true</active>
        <description>Invalid Org Id format</description>
        <errorConditionFormula>OR(not(begins( Customer_Org_ID__c ,&quot;00D&quot;)),AND( LEN(Customer_Org_ID__c) &lt;&gt;15))</errorConditionFormula>
        <errorDisplayField>Customer_Org_ID__c</errorDisplayField>
        <errorMessage>Invalid Org ID - Org IDs begin with 00D and are 15 characters in length.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OrgID_must_be_alphanumeric</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(REGEX(Customer_Org_ID__c , &quot;[a-zA-Z0-9]+&quot;))</errorConditionFormula>
        <errorDisplayField>Customer_Org_ID__c</errorDisplayField>
        <errorMessage>Org ID cannot contain special characters.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Valid_Canada_State</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
Customer_Country__c = &apos;CA&apos; , 
NOT(
CONTAINS(&apos;AB:BC:MB:NB:NL:NS:NT:NU:ON:PE:QC:SK:YT&apos;,  Customer_State__c )  
))</errorConditionFormula>
        <errorDisplayField>Customer_State__c</errorDisplayField>
        <errorMessage>Invalid State - State is not valid for country CA.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Valid_US_State</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
Customer_Country__c = &apos;US&apos; , 
NOT(
CONTAINS(&apos;AL:AK:AZ:AR:CA:CO:CT:DE:DC:FL:GA:HI:ID:IL:IN:IA:KS:KY:LA:ME:MD:MA:MI:MN:MS:MO:MT:NE:NV:NH:NJ:NM:NY:NC:ND:OH:OK:OR:PA:RI:SC:SD:TN:TX:UT:VT:VA:WA:WV:WI:WY:PR&apos;,  Customer_State__c )  
))</errorConditionFormula>
        <errorDisplayField>Customer_State__c</errorDisplayField>
        <errorMessage>Invalid State - State is not valid for Country US.</errorMessage>
    </validationRules>
