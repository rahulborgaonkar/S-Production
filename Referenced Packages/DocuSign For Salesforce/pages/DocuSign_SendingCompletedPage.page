<apex:page standardController="dsfs__DocuSign_Envelope__c"
	Extensions="dsfs.DocuSignEnvelopeController" tabStyle="dsfs__DocuSign_Status__c"
	standardStylesheets="true" ID="SendingCompleted">
	<apex:form >
		<apex:pageBlock title="{!CompleteMessage}" id="thePageBlock"
			mode="Edit" tabStyle="DocuSign_Status__c">
			<apex:pageBlockButtons >
				<apex:commandButton value="Return" action="{!DSReturnToSource}" />
			</apex:pageBlockButtons>
			<apex:actionRegion >
				<apex:pageBlockSection title="DocuSign Envelope Information"
					columns="1" collapsible="false" showHeader="false">
					<apex:pageBlockSectionItem >
						<apex:image url="{!$Resource.dsfs__DocuSignLogo}"
							title="DocuSign Send Complete." />
						<apex:outputpanel >
							<table cellpadding="3" cellspacing="0" border="0"
								style="font-size: 8pt">
								<tr>
									<td align="right"
										style="white-space: nowrap; font-weight: bold"><apex:outputLabel value="Transaction Information" />
									</td>
									<td><apex:outputLabel value="{!mySource}" />
									</td>
								</tr>
								<!--<tr><td align="right" style="white-space: nowrap; font-weight: bold"><apex:outputLabel value="Envelope" /></td>
                     <td><apex:outputLabel value="{!DSEnvelope.name}" /></td></tr>-->
								<tr>
									<td align="right"
										style="white-space: nowrap; font-weight: bold"><apex:outputLabel value="DocuSign Envelope ID" />
									</td>
									<td><apex:outputLabel value="{!DSEnvelope.dsfs__DocuSign_Envelope_ID__c}" />
									</td>
								</tr>
								<tr>
									<td align="right"
										style="white-space: nowrap; font-weight: bold"><apex:outputLabel value="Subject" />
									</td>
									<td><apex:outputLabel value="{!DSEnvelope.dsfs__DocuSign_Email_Subject__c}" />
									</td>
								</tr>
								<tr>
									<td align="right"
										style="white-space: nowrap; font-weight: bold"><apex:outputLabel value="Message" />
									</td>
									<td><apex:outputLabel value="{!DSEnvelope.dsfs__DocuSign_Email_Message__c}" />
									</td>
								</tr>
								<tr>
									<td align="right"
										style="white-space: nowrap; font-weight: bold"><apex:outputLabel value="Status" />
									</td>
									<td><apex:outputLabel value="{!MyTaggerResult}" />
									</td>
								</tr>
								<tr>
									<td align="right"
										style="white-space: nowrap; font-weight: bold"><apex:outputLabel value="Sign Now Status" rendered="{!IsSignNowStatusPage}" />
									</td>
									<td><apex:outputLabel value="{!MySignNowResult}"
											rendered="{!IsSignNowStatusPage}" />
									</td>
								</tr>
							</table>
						</apex:outputpanel>
					</apex:pageBlockSectionItem>
				</apex:pageBlockSection>
			</apex:actionRegion>
		</apex:pageBlock>
	</apex:form>
</apex:page>